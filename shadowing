public class shadowing {
    static int x = 90;
    public static void main(String[] args) {
        System.out.println(x);
        int x = 45;
        System.out.println(x);
        fun();
        System.out.println(x);

    }
    static void fun(){
        System.out.println(x);
        x = 20;
        System.out.println(x);
    }
}
